#Sementara
#Game menjumlahkan biilangan prima

stop = False
sum = 0
bilpertama = True

while stop == False:
    a = input()
    if a == "y":
        n = int(input())
        if n < 0:
            print("NoProceed")
            stop = True
        else:
            if n > 1:
                prima = True
                for i in range (2, int(n**0.5) + 1):
                    if n % i == 0:
                        prima = False
                        break
                if prima == True:
                    if bilpertama:
                        sum = n
                        bilpertama = False
                    else:
                        sum = sum - n
    elif a == 'n':
        if bilpertama:
            print(0)
        else:
            print(sum)
        stop = True
    else:
        print("NoProceed")
        stop = True
